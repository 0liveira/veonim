{
  "name": "veonim",
  "version": "0.1.0",
  "description": "vim end game",
  "main": "build/main.js",
  "scripts": {
    "fixexp": "sed -i '' -e 's/Object.defineProperty(exports, \"__esModule\", { value: true });//g' build/workers/*.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build",
    "typescript": "tsc -p tsconfig.json && tsc -p tsconfig.workers.json",
    "watch": "tsc -p tsconfig.json --watch",
    "build": "npm run typescript && npm run fixexp && cp src/index.html build/index.html",
    "rebuild": "npm run clean && npm run build",
    "r": "npm run rebuild && npm start",
    "start": "electron build/main.js",
    "gen-api": "node tools/gen-api.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deomitrus/veonim.git"
  },
  "author": "Deomitrus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deomitrus/veonim/issues"
  },
  "homepage": "https://github.com/deomitrus/veonim#readme",
  "dependencies": {
    "chokidar": "1.7.0",
    "fuzzaldrin-plus": "0.4.1",
    "glob": "7.1.2",
    "huu": "1.1.2",
    "hyperapp": "0.10.1",
    "msgpack-lite": "0.1.26",
    "through": "2.3.8"
  },
  "devDependencies": {
    "@types/chokidar": "1.7.1",
    "@types/fuzzaldrin-plus": "0.0.1",
    "@types/msgpack-lite": "0.1.4",
    "@types/node": "8.0.12",
    "@types/through": "0.0.28",
    "electron": "1.6.11",
    "require-reload": "0.2.2",
    "typescript": "2.4.2"
  }
}
